@model Dictionary<string, int>

<div id="tags-nav">
    @if(Model.Keys.Any()) {
        <ul>
            @foreach(var kvp in Model) {
            <li>@Html.RouteLink(string.Format("{0} ({1})", kvp.Key, kvp.Value), "Tag", new { tagName = kvp.Key.ToLowerInvariant(), })</li>
            }
        </ul>
    }
</div>