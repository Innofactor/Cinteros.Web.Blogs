@model System.Collections.Generic.IEnumerable<Blogger.DataSource.Model.BlogInfo>

<div id="blogs-nav">
    @if(Model.Any()) {
        <ul>
            @foreach(var blogInfo in Model) {
                <li>
                    <a href="@blogInfo.Url" title="@blogInfo.Subtitle">@blogInfo.Title</a>
                </li>
            }
        </ul>
    }
</div>